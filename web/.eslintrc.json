{
  "extends": [
    "airbnb",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "prettier"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, {"extensions": [".js"]}],
    "react/prop-types": 0,
    "react/destructuring-assignment": 0,
    "object-curly-newline": 0,
    "no-console": 0,
    "prettier/prettier": "error"
  }
}
